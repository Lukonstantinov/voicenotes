{
  "name": "musicians-scratchpad",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": {
            "strict": true
          }
        }
      ]
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^3.0.1",
    "expo": "~54.0.33",
    "expo-document-picker": "~13.0.0",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "private": true,
  "codegenConfig": {
    "name": "AudioPitchSpec",
    "type": "modules",
    "jsSrcsDir": "src/bridge",
    "android": {
      "javaPackageName": "com.dev.musicianscratchpad"
    }
  }
}
